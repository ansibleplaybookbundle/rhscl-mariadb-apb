---

apiVersion: v1
kind: Service
metadata:
  name: {{ app_name }}
  namespace : "{{ namespace | string }}"
  labels:
    app: {{ app_name }}
    service: {{ app_name }}
spec:
  selector:
    app: {{ app_name }}
    service: {{ app_name }}
  ports:
    - protocol: TCP
      port: {{ mariadb_port }}
      targetPort: {{mariadb_port }}
